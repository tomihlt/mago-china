{
  "expo": {
    "name": "mago-china",
    "slug": "mago-china",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/micon.jpeg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/micon.jpeg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tomihlt.magochina"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/micon.jpeg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/micon.jpeg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "permissions": [
      "CAMERA"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "60b0bbf0-6df6-481a-8f2b-b2914c6f2d72"
      }
    }
  }
}
